HTTP_ADDRESS=":8080"
ENABLE_HTTP_REDIRECT=

# Directory containing your .opus files (empty falls back to ./media)
MEDIA_DIR="media"

# Stream/station name used by WebRTC + UI defaults
STREAM_NAME="EggsFM"

# Frontend config for the Svelte app
VITE_API_BASE=
VITE_API_PATH="/api"
VITE_STATION_NAME="EggsFM"
VITE_PAGE_TITLE="EggsFM"
VITE_LISTEN_URL=

# something something firefox stun is stupid
STUN_SERVERS=stun.l.google.com:19302

# /etc/letsencrypt/live/<your-domain-name>/privkey.pem
SSL_KEY=

# /etc/letsencrypt/live/<your-domain-name>/fullchain.pem
SSL_CERT=
